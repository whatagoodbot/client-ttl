{
  "name": "client-rvrb",
  "version": "0.0.1",
  "description": "A bot to interact with users in chat on RVRB",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development nodemon -r dotenv/config src/index.js",
    "prod": "NODE_ENV=production nodemon -r dotenv/config src/index.js",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "start": "node src/index.js",
    "test": "npm run lint && nyc --reporter=html --reporter=text mocha"
  },
  "repository": {
    "type": "git",
    "url": "git@github-whatagoodbot:whatagoodbot/client-rvrb.git"
  },
  "keywords": [
    "bot",
