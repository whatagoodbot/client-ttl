  ],
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.3.6",
    "dotenv": "^16.0.2",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "nyc": "^15.1.0",
    "standard": "^17.0.0"
  },
  "dependencies": {
    "@whatagoodbot/rpc": "^1.0.6",
    "hot-shots": "^9.2.0",
    "message-broker": "github:whatagoodbot/message-broker#v3.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-syslog": "^2.6.0"
  }
